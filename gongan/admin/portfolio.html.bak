			  	type:"post",
			  	data: new FormData(this),
			  	contentType:false,
			  	processData:false,
			  	success: function(data){
			  		$("#gallery").html(data);
			  		alert("Image Uploaded");
			  	}
			  });		    
			    
			});
			
		});
